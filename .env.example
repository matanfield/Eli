# Expo Public Environment Variables (client-side)
EXPO_PUBLIC_API_BASE=https://your-vercel-app.vercel.app
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Development
EXPO_PUBLIC_DEV_MODE=true
EXPO_PUBLIC_LOG_LEVEL=debug

# Backend Environment Variables (server-side only)
# These should be set in Vercel dashboard, not in client env

# OpenAI
OPENAI_API_KEY=your-openai-api-key
REALTIME_MODEL=gpt-4o-realtime-preview

# Supabase (server-side)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
SUPABASE_ANON_KEY=your-supabase-anon-key

# ElevenLabs (future)
ELEVENLABS_API_KEY=your-elevenlabs-api-key
